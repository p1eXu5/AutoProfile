<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <LangVersion>9.0</LangVersion>
    <Nullable>enable</Nullable>
	  
	  <PublishRepositoryUrl>true</PublishRepositoryUrl>
	  
	  <EmbedUntrackedSources>true</EmbedUntrackedSources>
    <Version>10.1.5.2-beta1</Version>
    <Authors>p1eXu5</Authors>
    <Description>
		Additional attributes for AutoMapper.
		Package version consists of {AutoMapperVersion}.{MSDependencyInjection.Abstractions}.{MSLogging.Abstractions}
	</Description>
    <PackageTags>AutoMapper, mapper, attributes, c#, mapping, MapTo, MapFrom</PackageTags>
    <PackageProjectUrl>https://github.com/p1eXu5/p1eXu5.AutoProfile</PackageProjectUrl>
    <RepositoryUrl>https://github.com/p1eXu5/p1eXu5.AutoProfile</RepositoryUrl>
    <Copyright>2020, Vladimir Likhatskiy (p1eXu5)</Copyright>
    <PackageLicenseFile>LICENSE</PackageLicenseFile>
    <FileVersion>0.10.1.001</FileVersion>
    <Product>AutoProfile</Product>
    <PackageId>AutoProfile</PackageId>
	  <IncludeSymbols>true</IncludeSymbols>
    <SymbolPackageFormat>snupkg</SymbolPackageFormat>	      
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="AutoMapper" Version="10.1.1" />
    <PackageReference Include="Microsoft.Extensions.DependencyInjection.Abstractions" Version="5.0.0" />
    <PackageReference Include="Microsoft.Extensions.Logging.Abstractions" Version="2.2.0" />
    <PackageReference Include="Microsoft.SourceLink.GitHub" Version="1.1.1">
      <PrivateAssets>all</PrivateAssets>
      <IncludeAssets>runtime; build; native; contentfiles; analyzers; buildtransitive</IncludeAssets>
    </PackageReference>
  </ItemGroup>

  <ItemGroup>
    <None Include="..\..\LICENSE">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

</Project>
